{
    "name": "Soundweave",
    "title": "Soundweave",
    "author": "Antagnostic",
    "description": "A module for forwarding sound from the GM's machine to the rest of the party.",
    "version": "0.1.0",  
    "compatibility": {
        "minimum": "10.0.0",
        "verified": "10.2.1"
    },
    "homepage": "https://github.com/antagnostic/soundweave",
    "license": "MIT",
    "scripts": [
        "soundweave.js"
    ],
    "styles": [],
    "keywords": [
        "sound",
        "audio",
        "forwarding",
        "GM",
        "party"
    ],
    "dependencies": [
        {
            "name": "libWrapper",
            "manifest": "https://github.com/ruipin/fvtt-lib-wrapper/releases/latest/download/module.json"
        }
    ],
    "config": {
        "volume": {
            "name": "Volume",
            "hint": "Adjust the volume of the forwarded sound.",
            "scope": "world",
            "config": true,
            "default": 1,
            "type": "number",
            "range": {
                "min": 0,
                "max": 1,
                "step": 0.1
            }
        }
    }
}